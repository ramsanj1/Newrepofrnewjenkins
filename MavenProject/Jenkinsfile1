pipeline{
    agent any
    stages{
        stage ("build")
        {
         steps
         {
             echo " build a project."
             sh "cd MavenProject ; mvn clean install ; pwd"
         }
        }
        stage ("clean up")
        {
          steps
          {
              echo "cleaninig up workspace"
               cleanWs()
          }
        }
    }
}
